<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Our.Umbraco.Switcher</id>
    <version>1.0.3</version>
    <title>Switcher</title>
    <authors>Bjarne Fyrstenborg</authors>
    <owners>Bjarne Fyrstenborg</owners>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://github.com/bjarnef/Switcher</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/bjarnef/Switcher/master/assets/img/switcher-icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Simple property editor that works as an alternative to the core true/false datatype.</description>
    <tags>Umbraco, Toggle</tags>
    <releaseNotes>
      v1.0.3
      New: Nuget package build
      New: Updated colours and switch styles
      
      v1.0.2
      Fix: Set form $dirty property to true, so the editor see the prompt warning about unsaved changes.

      v1.0.1
      New: Localization support for labels
      Fix: Switch class conflicted with class on datepicker in Umbraco 7.1.x
      
      v1.0.0
      Initial release
    </releaseNotes>
  </metadata>
  <files>
    <file src="..\app\**\*.*" target="content\App_Plugins\Switcher" />
  </files>
</package>